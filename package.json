  "bugs": {
    "url": "https://github.com/shumphrey/rebase_tutorial/issues"
  },
  "homepage": "https://github.com/shumphrey/rebase_tutorial#readme",
  "dependencies": {
    "express": "^4.16.3",
    "pug": "^2.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/jest": "^22.1.3",
    "jest": "^22.0.4",
    "nodemon": "^1.13.0",
    "ts-jest": "^22.0.4",
    "tslint": "^5.11.0",
    "typescript": "^2.7.2"
  }
}
