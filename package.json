{
  "name": "rebase_tutorial",
  "version": "0.1.0",
  "description": "A tutorial for rebasing",
  "scripts": {
    "build": "npm run build-ts && npm run lint",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "npm run serve",
    "serve": "node dist/server.js",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shumphrey/rebase_tutorial.git"
  },
